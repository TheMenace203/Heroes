        MainnSection:NewDropdown("Destinations", "", {"Red Rose Coven Room", "Wolf Den", "Witch tunnel", "Human Spawn", "The Bar", "The Church", "The Stake Place", "Res Circle 1", "Res Circle 2",}, function(currentOption)
                        print(currentOption)

        if currentOption == "Red Rose Coven Room" then
                            
         game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame = CFrame.new(-376, -267.459564, -4881, 1, 0, 0, 0, 1, 0, 0, 0, 1)    
                            
