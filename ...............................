            MainSection:NewToggle("Teleport (T)", "ToggleInfo", function(state)
            if state then
            print("Toggle On")

            clicktpon = true                    
            plr = game.Players.LocalPlayer
            hum = plr.Character.HumanoidRootPart
            mouse = plr:GetMouse()
            mouse.KeyDown:connect(function(key)
            if key == 't' then
            if mouse.Target then
            if clicktpon then
            
                local args = {
                    [1] = "Magic",
                    [2] = {
                        [2] = Vector3.new(mouse.Hit.x, mouse.Hit.y + 5, mouse.Hit.z),
                        [1] = "Teleport"
                    }
                }
                
                game:GetService("ReplicatedStorage").Events.Witch:FireServer(unpack(args))
            end
            end
            end
